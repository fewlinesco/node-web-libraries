  "files": [
    "dist/**/*"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@fwl/web",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "ts-node index.ts",
    "prelint": "prettier --trailing-comma all --write './**/*.md'",
    "lint": "eslint --ext ts --ignore-pattern dist .",
    "prebuild": "yarn clean",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint",
    "preversion": "yarn lint",
    "test": "jest"
