{
  "author": "Fewlines",
  "description": "Tracing part of Fewlines Web Libraries",
  "dependencies": {
    "@fwl/logging": "0.1.2",
    "@opentelemetry/api": "1.0.2",
    "@opentelemetry/context-async-hooks": "0.25.0",
    "@opentelemetry/core": "0.25.0",
    "@opentelemetry/exporter-collector": "0.25.0",
    "@opentelemetry/exporter-zipkin": "0.25.0",
    "@opentelemetry/node": "0.24.0",
    "@opentelemetry/plugin-http": "0.18.2",
    "@opentelemetry/plugin-https": "0.18.2",
    "@opentelemetry/resources": "0.25.0",
    "@opentelemetry/semantic-conventions": "0.25.0",
    "@opentelemetry/tracing": "0.24.0",
    "@types/node": "16.7.2"
  },
  "devDependencies": {
    "@fewlines/eslint-config": "3.1.2",
    "@types/jest": "27.0.1",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-prettier": "3.4.1",
    "eslint": "7.32.0",
    "jest": "27.0.6",
    "prettier": "2.3.2",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.3.5"
  },
  "eslintConfig": {
    "extends": [
      "@fewlines/eslint-config/node",
      "@fewlines/eslint-config/typescript"
    ]
  },
  "files": [
    "dist/**/*"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "dist/"
    ]
  },
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@fwl/tracing",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "experimental-publish": "yarn publish --new-version \"0.0.0-experimental-$(git rev-parse --short HEAD)\" --access public --tag experimental",
    "prelint": "prettier --trailing-comma all --write './**/*.md'",
    "lint": "eslint --ext ts --ignore-pattern dist .",
    "prebuild": "yarn clean",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint",
    "preversion": "yarn lint",
    "test": "jest"
  },
  "types": "dist/index.d.ts",
  "version": "0.10.5"
}
