{
  "name": "@fwl/database",
  "author": "Fewlines",
  "description": "Database part of Fewlines Web Libraries",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "version": "0.6.4",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fewlinesco/node-web-libraries",
    "directory": "packages/database"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "experimental-publish": "yarn publish --new-version \"0.0.0-experimental-$(git rev-parse --short HEAD)\" --access public --tag experimental",
    "lint": "eslint --ext ts --ignore-pattern dist .",
    "prebuild": "yarn clean",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint",
    "preversion": "yarn lint",
    "test": "jest --runInBand --verbose"
  },
  "dependencies": {
    "@fwl/tracing": "0.10.7",
    "@types/node": "17.0.40",
    "@types/pg": "8.6.5",
    "@types/uuid": "8.3.4",
    "pg": "8.7.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@fewlines/eslint-config": "3.1.2",
    "@types/jest": "28.1.1",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "28.1.0",
    "prettier": "2.6.2",
    "ts-jest": "28.0.4",
    "ts-node": "10.8.1",
    "typescript": "4.7.3"
  },
  "eslintConfig": {
    "extends": [
      "@fewlines/eslint-config/node",
      "@fewlines/eslint-config/typescript"
    ]
  }
}
