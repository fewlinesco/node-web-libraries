{
  "author": "Fewlines",
  "description": "Database part of Fewlines Web Libraries",
  "dependencies": {
    "@fwl/tracing": "0.10.0",
    "pg": "8.6.0"
  },
  "devDependencies": {
    "@fewlines/eslint-config": "3.1.2",
    "@types/jest": "26.0.23",
    "@types/node": "15.6.1",
    "@types/pg": "8.6.0",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "27.0.1",
    "prettier": "2.3.0",
    "ts-jest": "27.0.1",
    "ts-node": "10.0.0",
    "typescript": "4.3.2"
  },
  "eslintConfig": {
    "extends": [
      "@fewlines/eslint-config/node",
      "@fewlines/eslint-config/typescript"
    ]
  },
  "files": [
    "dist/**/*"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@fwl/database",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "experimental-publish": "yarn publish --new-version \"0.0.0-experimental-$(git rev-parse --short HEAD)\" --access public --tag experimental",
    "lint": "eslint --ext ts --ignore-pattern dist .",
    "prebuild": "yarn clean",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint",
    "preversion": "yarn lint",
    "test": "jest --runInBand --verbose"
  },
  "types": "dist/index.d.ts",
  "version": "0.5.2"
}
